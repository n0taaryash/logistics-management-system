{
  "version": 2,
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.5.2"
    },
    "*.php": {
      "runtime": "vercel-php@0.5.2"
    }
  },
  "routes": [
    { "src": "/api/(.*)\\.php", "dest": "/api/$1.php" },
    { "src": "/(.*)\\.php", "dest": "/$1.php" },
    { "src": "/(.*)", "dest": "/index.php" }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false"
  }
}
